# BGPq4 Configuration
BGPQ4_BINARY=/usr/bin/bgpq4
IRR_SOURCES=RIPE,RADB,ARIN

# Timing Configuration
SYNC_TIMEOUT_MS=1000
MAX_EXECUTION_TIME_MS=30000

# Retry Configuration
MAX_RETRIES=3
RETRY_BACKOFF_FACTOR=2.0

# Cache Configuration
DEFAULT_CACHE_TTL=300
MAX_CACHE_TTL=3600

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
JOB_RESULT_TTL=3600

# API Configuration
API_TITLE=FastBGPQ4
API_VERSION=v1
LOG_LEVEL=INFO
